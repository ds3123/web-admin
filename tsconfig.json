{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [ "jest" , "cypress" , "@testing-library/jest-dom" , "@testing-library/cypress"] ,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@layout/*": [
        "components/layout/*"
      ],
      "@controller/*": [
        "components/controller/*"
      ],
      "@service/*": [
        "components/service/*"
      ],
      "@data/*": [
        "components/data/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@schema/*": [
        "utils/schemas/*"
      ],
      "@api/*": [
        "utils/api/*"
      ],
      "@react-query/*": [
        "utils/react-query/*"
      ],
      "@rq_hooks/*": [
        "utils/react-query/hooks/*"
      ],
      "@rq_keys/*": [
        "utils/react-query/keys/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@contexts/*": [
        "contexts/*"
      ],
      "@store/*": [
        "store/*"
      ],
      "@reducer/*": [
        "store/reducers/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx" ,    
  ],
  "exclude": [
    "node_modules" ,

    "./cypress.config.ts",
    "cypress",
    
  ]
}
